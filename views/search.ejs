<html>
<body>
    <button><a href="/main">Home</a></button>
    <br>
    <table>
        <% for(var i = 0;i < book.length;i++){ %>
            <tr>
            <%= book[i].book_name %>
            <%= book[i].book_tag %>
            <%= book[i].book_description %>
            <%= book[i].book_price %>
            rate : <%= book[i].book_rate %>
            view : <%= book[i].book_view %>
            <form action="/api/book" method="get">
                <input type="hidden" name='id' value="<%= book[i]._id %>">
                <button type="submit">get</button>
            </form>
            <br>
            </tr>
        <% } %>
    </table>
</body>
</html>